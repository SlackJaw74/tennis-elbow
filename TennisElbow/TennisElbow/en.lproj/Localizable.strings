// MARK: - Activity Instructions
"activity.wrist_extension_stretch.instruction.1" = "Extend your arm in front with palm facing down";
"activity.wrist_extension_stretch.instruction.2" = "Use your other hand to gently pull fingers back";
"activity.wrist_extension_stretch.instruction.3" = "Hold for 15-30 seconds";
"activity.wrist_extension_stretch.instruction.4" = "Repeat on both arms";

"activity.wrist_flexion_stretch.instruction.1" = "Extend your arm with palm facing up";
"activity.wrist_flexion_stretch.instruction.2" = "Gently pull fingers down with other hand";
"activity.wrist_flexion_stretch.instruction.3" = "Hold for 15-30 seconds";
"activity.wrist_flexion_stretch.instruction.4" = "Feel the stretch in your forearm";

