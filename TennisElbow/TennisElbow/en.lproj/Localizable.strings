"activity.grip_strengthening.instruction.3" = "Release slowly";
"activity.grip_strengthening.instruction.4" = "Rest between sets";

"activity.ice_therapy.instruction.1" = "Apply ice pack to outer elbow";
"activity.ice_therapy.instruction.2" = "Use a towel to protect skin";
"activity.ice_therapy.instruction.3" = "Apply for 15 minutes";
"activity.ice_therapy.instruction.4" = "Repeat 3-4 times daily as needed";

"activity.forearm_massage.instruction.1" = "Use your thumb to apply gentle pressure";
"activity.forearm_massage.instruction.2" = "Massage along the forearm muscles";
"activity.forearm_massage.instruction.3" = "Focus on tender points";
"activity.forearm_massage.instruction.4" = "Move slowly and breathe deeply";

