// MARK: - Activity Instructions
"activity.wrist_extension_stretch.instruction.1" = "Extienda el brazo frente a usted con la palma hacia abajo";
"activity.wrist_extension_stretch.instruction.2" = "Use la otra mano para tirar suavemente los dedos hacia atrás";
"activity.wrist_extension_stretch.instruction.3" = "Mantenga durante 15-30 segundos";
"activity.wrist_extension_stretch.instruction.4" = "Repita en ambos brazos";

"activity.wrist_flexion_stretch.instruction.1" = "Extienda el brazo con la palma hacia arriba";
"activity.wrist_flexion_stretch.instruction.2" = "Tire suavemente los dedos hacia abajo con la otra mano";
"activity.wrist_flexion_stretch.instruction.3" = "Mantenga durante 15-30 segundos";
"activity.wrist_flexion_stretch.instruction.4" = "Sienta el estiramiento en su antebrazo";

"activity.eccentric_wrist_extension.instruction.1" = "Apoye el antebrazo en una mesa, con la palma hacia abajo";
"activity.eccentric_wrist_extension.instruction.2" = "Sostenga un peso ligero (comience con 1-2 lbs)";
"activity.eccentric_wrist_extension.instruction.3" = "Baje lentamente el peso doblando la muñeca hacia abajo";
"activity.eccentric_wrist_extension.instruction.4" = "Use la otra mano para ayudar a levantarlo nuevamente";
"activity.eccentric_wrist_extension.instruction.5" = "Concéntrese en el movimiento de descenso";

"activity.grip_strengthening.instruction.1" = "Use una pelota antiestrés o fortalecedor de agarre";
"activity.grip_strengthening.instruction.2" = "Apriete firmemente durante 5 segundos";
"activity.grip_strengthening.instruction.3" = "Suelte lentamente";
"activity.grip_strengthening.instruction.4" = "Descanse entre series";

"activity.ice_therapy.instruction.1" = "Aplique una bolsa de hielo en la parte externa del codo";
"activity.ice_therapy.instruction.2" = "Use una toalla para proteger la piel";
"activity.ice_therapy.instruction.3" = "Aplique durante 15 minutos";
"activity.ice_therapy.instruction.4" = "Repita 3-4 veces al día según sea necesario";

"activity.forearm_massage.instruction.1" = "Use el pulgar para aplicar presión suave";
"activity.forearm_massage.instruction.2" = "Masajee a lo largo de los músculos del antebrazo";
"activity.forearm_massage.instruction.3" = "Concéntrese en los puntos sensibles";
"activity.forearm_massage.instruction.4" = "Muévase lentamente y respire profundamente";

"activity.wrist_rotations.instruction.1" = "Sostenga el brazo con el codo doblado a 90 grados";
"activity.wrist_rotations.instruction.2" = "Rote lentamente la muñeca en círculos";
"activity.wrist_rotations.instruction.3" = "Haga 10 rotaciones en sentido horario";
"activity.wrist_rotations.instruction.4" = "Luego 10 rotaciones en sentido antihorario";

"activity.pain_level_check.instruction.1" = "Califique su nivel de dolor actual";
"activity.pain_level_check.instruction.2" = "Considere el dolor durante las actividades";
"activity.pain_level_check.instruction.3" = "Anote cualquier cambio de sesiones anteriores";
"activity.pain_level_check.instruction.4" = "Esto ayuda a rastrear el progreso de su recuperación";

