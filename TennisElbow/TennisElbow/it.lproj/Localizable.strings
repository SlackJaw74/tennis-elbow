// MARK: - Activity Instructions
"activity.wrist_extension_stretch.instruction.1" = "Estendi il braccio davanti a te con il palmo rivolto verso il basso";
"activity.wrist_extension_stretch.instruction.2" = "Usa l'altra mano per tirare delicatamente le dita all'indietro";
"activity.wrist_extension_stretch.instruction.3" = "Mantieni la posizione per 15-30 secondi";
"activity.wrist_extension_stretch.instruction.4" = "Ripeti su entrambe le braccia";

"activity.wrist_flexion_stretch.instruction.1" = "Estendi il braccio con il palmo rivolto verso l'alto";
"activity.wrist_flexion_stretch.instruction.2" = "Tira delicatamente le dita verso il basso con l'altra mano";
"activity.wrist_flexion_stretch.instruction.3" = "Mantieni la posizione per 15-30 secondi";
"activity.wrist_flexion_stretch.instruction.4" = "Senti l'allungamento nell'avambraccio";

"activity.eccentric_wrist_extension.instruction.1" = "Appoggia l'avambraccio su un tavolo, palmo verso il basso";
"activity.eccentric_wrist_extension.instruction.2" = "Tieni un peso leggero (inizia con 0,5-1 kg)";
"activity.eccentric_wrist_extension.instruction.3" = "Abbassa lentamente il peso piegando il polso verso il basso";
"activity.eccentric_wrist_extension.instruction.4" = "Usa l'altra mano per aiutarti a sollevarlo";
"activity.eccentric_wrist_extension.instruction.5" = "Concentrati sul movimento di abbassamento";

"activity.grip_strengthening.instruction.1" = "Usa una palla anti-stress o un rafforzatore per la presa";
"activity.grip_strengthening.instruction.2" = "Stringi con forza per 5 secondi";
"activity.grip_strengthening.instruction.3" = "Rilascia lentamente";
"activity.grip_strengthening.instruction.4" = "Riposa tra le serie";

"activity.ice_therapy.instruction.1" = "Applica un impacco di ghiaccio sul gomito esterno";
"activity.ice_therapy.instruction.2" = "Usa un asciugamano per proteggere la pelle";
"activity.ice_therapy.instruction.3" = "Applica per 15 minuti";
"activity.ice_therapy.instruction.4" = "Ripeti 3-4 volte al giorno secondo necessità";

"activity.forearm_massage.instruction.1" = "Usa il pollice per applicare una pressione delicata";
"activity.forearm_massage.instruction.2" = "Massaggia lungo i muscoli dell'avambraccio";
"activity.forearm_massage.instruction.3" = "Concentrati sui punti dolenti";
"activity.forearm_massage.instruction.4" = "Muoviti lentamente e respira profondamente";

"activity.wrist_rotations.instruction.1" = "Tieni il braccio esteso con il gomito piegato a 90 gradi";
"activity.wrist_rotations.instruction.2" = "Ruota lentamente il polso in cerchio";
"activity.wrist_rotations.instruction.3" = "Fai 10 rotazioni in senso orario";
"activity.wrist_rotations.instruction.4" = "Poi 10 rotazioni in senso antiorario";

"activity.pain_level_check.instruction.1" = "Valuta il tuo livello di dolore attuale";
"activity.pain_level_check.instruction.2" = "Considera il dolore durante le attività";
"activity.pain_level_check.instruction.3" = "Annota eventuali cambiamenti rispetto alle sessioni precedenti";
"activity.pain_level_check.instruction.4" = "Questo aiuta a monitorare i tuoi progressi di recupero";

