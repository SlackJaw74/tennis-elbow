// MARK: - AboutTennisElbowView
"What is Tennis Elbow?" = "Cos'è il Gomito del Tennista?";
"about.tennis_elbow_description" = "Il gomito del tennista (epicondilite laterale) è una condizione dolorosa che si verifica quando i tendini del gomito sono sovraccaricati, solitamente da movimenti ripetitivi del polso e del braccio. Nonostante il nome, la maggior parte delle persone che sviluppano il gomito del tennista non gioca a tennis. È comune nelle persone i cui lavori comportano movimenti ripetitivi del braccio.";
"Common Symptoms" = "Sintomi Comuni";
"symptoms.pain_outer_part" = "Dolore o bruciore sulla parte esterna del gomito";
"symptoms.weak_grip" = "Forza di presa debole";
"symptoms.pain_lifting" = "Dolore quando si solleva o si piega il braccio";
"symptoms.pain_gripping" = "Dolore quando si afferrano oggetti";
"Treatment Approach" = "Approccio Terapeutico";
"about.treatment_description" = "Questa app segue protocolli di trattamento basati sull'evidenza che combinano riposo, terapia del freddo, stretching ed esercizi di rafforzamento. Gli esercizi sono progettati per sviluppare gradualmente la forza nei muscoli e tendini dell'avambraccio. La maggior parte delle persone vede miglioramenti entro 6-12 settimane di trattamento costante.";
"View All Medical Sources" = "Visualizza Tutte le Fonti Mediche";

// MARK: - TipsView
"Ice Regularly" = "Applicare Ghiaccio Regolarmente";
"tips.ice_description" = "Applica ghiaccio per 15 minuti, 3-4 volte al giorno per ridurre l'infiammazione";
"Start Slow" = "Inizia Lentamente";
"tips.start_slow_description" = "Inizia con allungamenti delicati e aumenta gradualmente l'intensità";
"Be Consistent" = "Sii Costante";
"tips.consistency_description" = "Esercizi regolari e quotidiani sono più efficaci di sessioni intense occasionali";
"Modify Activities" = "Modifica le Attività";
"tips.modify_description" = "Evita o modifica le attività che aggravano i tuoi sintomi";
"Warm Up" = "Riscaldamento";
"tips.warmup_description" = "Riscalda sempre il braccio prima degli esercizi";
"Seek Professional Help" = "Cerca Aiuto Professionale";
"tips.professional_help_description" = "Se il dolore persiste oltre le 6-8 settimane, consulta un operatore sanitario";
"Sources" = "Fonti";
"tips.sources_description" = "Le informazioni sopra si basano sulle linee guida dell'American Academy of Orthopaedic Surgeons (AAOS) e sui protocolli di trattamento della Mayo Clinic per l'epicondilite laterale.";

